	<excludes>
		<pattern>/img/*</pattern>
		<pattern>/css/*</pattern>
		<pattern>/js/*</pattern>
		<pattern>/script/*</pattern>
		<pattern>*/ajax</pattern>
		<pattern>/login</pattern>
		<pattern>/login/*</pattern>
    </excludes>
